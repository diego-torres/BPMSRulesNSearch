package org.acme.cloud_solution_projects;

import java.lang.Number;

rule "view-cloudSolution"
	dialect "mvel"
	ruleflow-group "view-rules"
	no-loop true
	when
		project : Project( )
		not (CloudSolution( )) 
		view : ViewRecommendation( )
	then
		modify( project ) {
				setInformationCompleted( false )
		}
		modify( view ) {
				setViewName( "projects/" + project.id + "/cloud" )
		}
end
