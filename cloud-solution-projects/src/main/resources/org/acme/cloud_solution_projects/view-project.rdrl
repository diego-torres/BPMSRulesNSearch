package org.acme.cloud_solution_projects;

import java.lang.Number;

rule "view-project"
	dialect "mvel"
	no-loop true
	ruleflow-group "view-rules"
	when
		not (Project( )) 
	then
		ViewRecommendation view = new ViewRecommendation();
		view.setRecommendation( "Add Project information" );
		view.setViewName( "\"projects/add\"" );
		insert( view );
end
